{
    "env": {
      "browser": true,
      "es6": true,
      "commonjs": true,
      "jest": true
    },
    "extends": "airbnb",
    "parserOptions": {
      "ecmaVersion": 9,
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "react/prop-types": 0,
      "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
      "import/prefer-default-export": 0
    }
}